				<el>8</el>
			</start>
			<colIdx>
				<el>0</el>
				<el>1</el>
				<el>0</el>
				<el>1</el>
				<el>0</el>
				<el>1</el>
				<el>0</el>
				<el>1</el>
			</colIdx>
			<value>
				<el>.7</el>
				<el>1.</el>	
				<el>.5</el>
				<el>0.8333</el>
				<el>1.0</el>
				<el>0.6667</el>
				<el>.1</el>
				<el>0.25</el>
			</value>
	</linearConstraintCoefficients>
	</instanceData>
</osil>
